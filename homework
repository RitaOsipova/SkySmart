a = int(input())

if a % 2 == 0:
  print('1')

else:
  print('0')

a = int(input())

if a == 1:
  print('I')
  
elif a == 2:
  print('II')

elif a == 3:
  print('III')

elif a == 4:
  print('IV')
elif a == 5:
  print('V')

elif a == 6:
  print('VI')

elif a == 7:
  print('VII')

elif a == 8:
  print('VIII')
  
elif a == 9:
  print('IX')

elif a == 10:
  print('X')

else:
  print('ERROR')

s = input()
a = len(s)
######################################################################
if a == 4:
  if s.isdigit():
    print('Correct pincode')
  else:
    print('Incorrect pincode')
else:
  print('Incorrect pincode')

a = int(input())
b = int(input())
######################################################################
if a % 2 == 0 or b % 2 == 0:
  print((a * 10) + b)

else:
  print(a + b)

mystring = input()
######################################################################
rev_str = reversed(mystring)
if list(mystring) == list(rev_str):
   print("1")
else:
   print("0")